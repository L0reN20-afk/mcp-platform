  try {
    // Using a free IP geolocation service
    const response = await fetch(`http://ip-api.com/json/${ip}`)
    const data = await response.json()
    return data.country || 'Unknown'
  } catch (error) {
    console.error('Error getting country from IP:', error)
    return 'Unknown'
  }
}


